{
  "title": "产品详情",
  "template": "info",
  "toolbar": [
    {
      "icon": "edit",
      "type": "button",
      "label": "编辑",
      "action": {
        "type": "page",
        "app": "iot",
        "page": "product-edit",
        "params_func": "return {id: data.id}"
      }
    },
    {
      "icon": "build",
      "type": "button",
      "label": "物模型",
      "action": {
        "type": "page",
        "app": "iot",
        "page": "product-model",
        "params_func": "return {id: data.id}"
      }
    },
    {
      "icon": "delete",
      "type": "button",
      "label": "删除",
      "confirm": "确认删除？",
      "action": {
        "type": "script",
        "script": "this.request.get('iot/product/'+data.id+'/delete').subscribe(res=>{this.navigate('/page/iot/product')})"
      }
    }
  ],
  "items": [
    {
      "key": "id",
      "label": "ID"
    },
    {
      "key": "name",
      "label": "名称"
    },
    {
      "key": "description",
      "label": "说明"
    },
    {
      "key": "type",
      "label": "类型"
    },
    {
      "key": "version",
      "label": "版本"
    },
    {
      "key": "disabled",
      "label": "禁用",
      "type": "boolean"
    }
  ],
  "data_api": "iot/product/:id",
  "children": [
    {
      "params_func": "return {id: data.id}",
      "content": {
        "title": "协议配置文件",
        "template": "table",
        "operators": [
          {
            "label": "配置文件",
            "action": {
              "type": "page",
              "app": "iot",
              "page":"product-config",
              "params_func": "return {config:data.name, product:this.params.id}"
            }
          }
        ],
        "columns": [
          {
            "key": "name",
            "label": "名称"
          },
          {
            "key": "description",
            "label": "说明"
          }
        ],
        "data_api": "iot/protocol/list"
      }
    },
    {
      "content": {
        "title": "设备列表",
        "template": "table"
      }
    }
  ]
}