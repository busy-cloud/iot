{
  "title": "产品详情",
  "template": "info",
  "toolbar": [
    {
      "icon": "edit",
      "type": "button",
      "label": "编辑",
      "action": {
        "type": "page",
        "app": "iot",
        "page": "product-edit",
        "params_func": "return {id: data.id}"
      }
    },
    {
      "icon": "build",
      "type": "button",
      "label": "产品物模型",
      "action": {
        "type": "page",
        "app": "iot",
        "page": "product-model",
        "params_func": "return {id: data.id}"
      }
    },
    {
      "icon": "api",
      "type": "button",
      "label": "产品协议配置",
      "action": {
        "type": "page",
        "app": "iot",
        "page": "product-config",
        "params_func": "return {product: data.id, config: data.protocol}"
      }
    },
    {
      "icon": "delete",
      "type": "button",
      "label": "删除",
      "confirm": "确认删除？",
      "action": {
        "type": "script",
        "script": "this.request.get('iot/product/'+data.id+'/delete').subscribe(res=>{this.navigate('/page/iot/product')})"
      }
    }
  ],
  "items": [
    {
      "key": "id",
      "label": "ID"
    },
    {
      "key": "name",
      "label": "名称"
    },
    {
      "key": "description",
      "label": "说明"
    },
    {
      "key": "type",
      "label": "类型"
    },
    {
      "key": "version",
      "label": "版本"
    },
    {
      "key": "protocol",
      "label": "协议"
    },
    {
      "key": "disabled",
      "label": "禁用",
      "type": "boolean"
    }
  ],
  "load_api": "iot/product/:id",
  "children": [
    {
      "app": "iot",
      "page": "product-device",
      "params_func": "return {product_id: params.id}"
    }
  ]
}