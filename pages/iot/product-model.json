{
  "title": "编辑物模型",
  "template": "form",
  "fields": [
    {
      "key": "properties",
      "label": "属性表",
      "type": "table",
      "children": [
        {
          "key": "name",
          "label": "变量",
          "type": "text"
        },
        {
          "key": "label",
          "label": "显示名称",
          "type": "text"
        },
        {
          "key": "unit",
          "label": "数据单位",
          "type": "text"
        },
        {
          "key": "type",
          "label": "数据类型",
          "type": "select",
          "default": "number",
          "options": [
            {
              "label": "布尔",
              "value": "bool"
            },
            {
              "label": "字符串",
              "value": "string"
            },
            {
              "label": "数值",
              "value": "number"
            },
            {
              "label": "数组",
              "value": "array"
            },
            {
              "label": "对象",
              "value": "object"
            }
          ]
        },
        {
          "key": "precision",
          "label": "精度",
          "type": "number",
          "default": 0
        }
      ]
    },
    {
      "key": "validators",
      "label": "属性检查",
      "type": "table",
      "children": [
        {
          "key": "expression",
          "label": "表达式",
          "type": "text"
        },
        {
          "key": "title",
          "label": "报警标题",
          "type": "text"
        },
        {
          "key": "message",
          "label": "报警内容",
          "type": "text"
        },
        {
          "key": "level",
          "label": "报警等级",
          "type": "select",
          "default": 3,
          "options": [
            {
              "label": "一级",
              "value": 1
            },
            {
              "label": "二级",
              "value": 2
            },
            {
              "label": "三级",
              "value": 3
            },
            {
              "label": "四级",
              "value": 4
            },
            {
              "label": "五级",
              "value": 5
            }
          ]
        },
        {
          "key": "delay",
          "type": "number",
          "label": "延迟报警s",
          "default": 60
        },
        {
          "key": "reset",
          "type": "number",
          "label": "报警重置s",
          "default": 0
        },
        {
          "key": "reset_times",
          "type": "number",
          "label": "报警重置次数",
          "default": 0
        },
        {
          "key": "disabled",
          "label": "禁用",
          "type": "switch"
        }
      ]
    }
  ],
  "load_api": "iot/product/:id/model",
  "submit_api": "iot/product/:id/model",
  "submit_success": "this.navigate('/page/iot/product-detail?id='+data.id)",
  "methods": {
  }
}