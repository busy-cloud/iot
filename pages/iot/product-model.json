{
  "title": "编辑物模型",
  "template": "form",
  "fields": [
    {
      "key": "properties",
      "label": "属性表",
      "type": "table",
      "children": [
        {
          "key": "name",
          "label": "变量",
          "type": "text"
        },
        {
          "key": "label",
          "label": "显示名称",
          "type": "text"
        },
        {
          "key": "unit",
          "label": "数据单位",
          "type": "text"
        },
        {
          "key": "type",
          "label": "数据类型",
          "type": "select",
          "default": "number",
          "options": [
            {"label": "布尔","value": "bool"},
            {"label": "字符串","value": "string"},
            {"label": "数值","value": "number"},
            {"label": "数组","value": "array"},
            {"label": "对象","value": "object"}
          ]
        },
        {
          "key": "precision",
          "label": "精度",
          "type": "number",
          "default": 0
        }
      ]
    }
  ],
  "data_api": "iot/product/:id/model",
  "submit_api": "iot/product/:id/model",
  "submit_success": "this.navigate('/page/iot/product-detail?id='+data.id)",
  "methods": {
  }
}