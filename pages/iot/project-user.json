{
  "title": "项目用户",
  "template": "table",
  "toolbar": [
    {
      "label": "绑定",
      "icon": "plus",
      "type": "button",
      "action": {
        "type": "dialog",
        "app": "user",
        "page": "choose",
        "after_close": "this.request.get('iot/project/'+this.params.project_id+'/user/'+result.id+'/bind').subscribe(res=>this.refresh())"
      }
    }
  ],
  "operators": [
    {
      "icon": "delete",
      "title": "解绑",
      "confirm": "确认解绑？",
      "action": {
        "type": "script",
        "script": "this.request.get('iot/project/'+data.project_id+'/user/'+data.user_id+'/unbind').subscribe(res=>this.refresh())"
      }
    }
  ],
  "columns": [
    {
      "key": "user_id",
      "label": "用户ID",
      "action": {
        "type": "page",
        "app": "iot",
        "page": "user-detail",
        "params_func": "return {id: data.user_id}"
      }
    },
    {
      "key": "name",
      "label": "名称"
    },
    {
      "key": "created",
      "label": "日期",
      "type": "date"
    }
  ],
  "load_api": "iot/project/:project_id/user/list?limit=99999"
}