{
  "title": "设备",
  "template": "table",
  "toolbar": [
    {
      "key": "keyword",
      "type": "text",
      "placeholder": "请输入关键字"
    },
    {
      "type": "button",
      "icon": "search",
      "label": "搜索",
      "action": {
        "type": "script",
        "script": "this.keyword=this.toolbar.value.keyword; this.search()"
      }
    }
  ],
  "keywords": [
    "id",
    "name",
    "description"
  ],
  "operators": [
    {
      "icon": "check",
      "label": "选择",
      "action": {
        "type": "script",
        "script": "this.modelRef.close(data)"
      }
    }
  ],
  "columns": [
    {
      "key": "id",
      "label": "ID"
    },
    {
      "key": "name",
      "label": "名称"
    },
    {
      "key": "description",
      "label": "说明"
    },
    {
      "key": "station",
      "label": "地址",
      "type": "json"
    },
    {
      "key": "disabled",
      "label": "禁用",
      "type": "boolean"
    },
    {
      "key": "created",
      "label": "日期",
      "type": "date"
    }
  ],
  "search_api": "iot/device/search"
}