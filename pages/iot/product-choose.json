{
  "title": "产品",
  "template": "table",
  "toolbar": [
    {
      "key": "keyword",
      "type": "text",
      "placeholder": "请输入关键字"
    },
    {
      "type": "button",
      "icon": "search",
      "label": "搜索",
      "action": {
        "type": "script",
        "script": "this.keyword=this.toolbar.value.keyword; this.search()"
      }
    }
  ],
  "keywords": [
    "id",
    "name",
    "description"
  ],
  "operators": [
    {
      "icon": "check",
      "label": "选择",
      "action": {
        "type": "script",
        "script": "this.modelRef.close(data)"
      }
    }
  ],
  "columns": [
    {
      "key": "id",
      "label": "ID",
      "action": {
        "type": "page",
        "app": "iot",
        "page": "product-detail",
        "params_func": "return {id: data.id}"
      }
    },
    {
      "key": "name",
      "label": "名称"
    },
    {
      "key": "version",
      "label": "版本"
    }
  ],
  "search_api": "iot/product/search"
}