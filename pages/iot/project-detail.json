{
  "title": "项目详情",
  "template": "info",
  "toolbar": [
    {
      "icon": "edit",
      "type": "button",
      "label": "编辑",
      "action": {
        "type": "page",
        "app": "iot",
        "page": "project-edit",
        "params_func": "return {id: data.id}"
      }
    },
    {
      "icon": "delete",
      "type": "button",
      "label": "删除",
      "confirm": "确认删除？",
      "action": {
        "type": "script",
        "script": "this.request.get('iot/project/'+data.id+'/delete').subscribe(res=>{this.navigate('/page/iot/project')})"
      }
    }
  ],
  "items": [
    {
      "key": "id",
      "label": "ID"
    },
    {
      "key": "name",
      "label": "名称"
    },
    {
      "key": "description",
      "label": "说明"
    },
    {
      "key": "disabled",
      "label": "禁用"
    }
  ],
  "load_api": "iot/project/:id",
  "children": [
    {
      "app": "iot",
      "page": "space",
      "params_func": "return {project_id: params.id}"
    },
    {
      "app": "iot",
      "page": "project-device",
      "params_func": "return {project_id: params.id}"
    },
    {
      "app": "iot",
      "page": "project-user",
      "params_func": "return {project_id: params.id}"
    }
  ]
}