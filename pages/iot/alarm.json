{
  "title": "报警日志",
  "template": "table",
  "toolbar": [
    {
      "key": "keyword",
      "type": "text",
      "placeholder": "请输入关键字"
    },
    {
      "type": "button",
      "icon": "search",
      "label": "搜索",
      "action": {
        "type": "script",
        "script": "this.keyword=this.toolbar.value.keyword; this.search()"
      }
    }
  ],
  "keywords": [
    "device_id",
    "name",
    "description"
  ],
  "operators": [
    {
      "icon": "delete",
      "title": "删除",
      "confirm": "确认删除？",
      "action": {
        "type": "script",
        "script": "this.request.get('iot/alarm/'+data.id+'/delete').subscribe(res=>{this.refresh()})"
      }
    }
  ],
  "columns": [
    {
      "key": "id",
      "label": "ID",
      "action": {
        "type": "page",
        "app": "iot",
        "page": "device-detail",
        "params_func": "return {id: data.id}"
      }
    },
    {
      "key": "device_id",
      "label": "设备ID",
      "action": {
        "type": "page",
        "app": "iot",
        "page": "device-detail",
        "params_func": "return {id: data.device_id}"
      }
    },
    {
      "key": "project_id",
      "label": "项目ID",
      "action": {
        "type": "page",
        "app": "iot",
        "page": "project-detail",
        "params_func": "return {id: data.project_id}"
      }
    },
    {
      "key": "title",
      "label": "标题"
    },
    {
      "key": "message",
      "label": "消息"
    },
    {
      "key": "level",
      "label": "等级"
    },
    {
      "key": "created",
      "label": "日期",
      "type": "date"
    }
  ],
  "search_api": "iot/alarm/search"
}