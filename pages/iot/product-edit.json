{
  "title": "编辑产品",
  "template": "form",
  "fields": [
    {
      "key": "id",
      "label": "ID",
      "type": "text",
      "required": true
    },
    {
      "key": "name",
      "label": "名称",
      "type": "text",
      "required": true
    },
    {
      "key": "description",
      "label": "说明",
      "type": "text"
    },
    {
      "key": "type",
      "label": "类型",
      "type": "text"
    },
    {
      "key": "version",
      "label": "版本",
      "type": "text"
    },
    {
      "key": "protocol",
      "label": "协议",
      "type": "select",
      "options": []
    },
    {
      "key": "disabled",
      "label": "禁用",
      "type": "switch"
    }
  ],
  "load_api": "iot/product/:id",
  "submit_api": "iot/product/:id",
  "submit_success": "this.navigate('/page/iot/product-detail?id='+data.id)",
  "mount": "this.load_protocols()",
  "methods": {
    "load_protocols": "this.request.get('iot/protocol/list').subscribe(res=>{this.content.fields[5].options=res.data.map(d=>{return {value:d.name,label:d.description}}); })"
  }
}